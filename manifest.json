{
    "manifest_version": 3,
    "name": "Auto Login-CSES",
    "version": "1.0",
    "description": "This extension will automatically login to CSES",
    "permissions": [ "activeTab", "storage" ],
    "content_scripts": [
        {
            "matches": ["https://cses.fi/*"],
            "js": ["content.js"]
        }
    ]
}